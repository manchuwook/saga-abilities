{
  "servers": {
    "nx-mcp": {
      "type": "http",
      "url": "http://localhost:9429/mcp"
    },
    "my-development-projects": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "X:\\dev\\"]
    },
    "cloudflare-observability": {
      "command": "npx",
      "args": ["mcp-remote", "https://observability.mcp.cloudflare.com/sse"]
    },
    "cloudflare-analytics": {
      "command": "npx",
      "args": ["mcp-remote", "https://dns-analytics.mcp.cloudflare.com/sse"]
    },
    "cloudflare-docs": {
      "command": "npx",
      "args": ["mcp-remote", "https://docs.mcp.cloudflare.com/sse"]
    },
    "mantine": {
      "command": "npx",
      "args": ["@hakxel/mantine-ui-server"],
      "env": {
        "MANTINE_VERSION": "7.17.7" // Optional: specify version
      }
    }
  }
}
