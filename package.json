{
  "name": "saga-abilities",
  "version": "1.0.0",
  "description": "An ability manager for SAGA TTRPG",
  "type": "module",
  "scripts": {
    "dev": "nx serve",
    "build": "nx vite:build --config vitest.config.ts",
    "build:analyze": "cross-env ANALYZE=true nx vite:build --config vitest.config.ts",
    "build:prod": "pnpm clean && nx vite:build --config vitest.config.ts",
    "build:profile": "cross-env PROFILE=true pnpm build:prod",
    "build:measure": "cross-env MEASURE=true pnpm build:prod",
    "deploy": "pnpm clean && pnpm build:prod && wrangler pages deploy --project-name saga-abilities dist",
    "preview": "nx vite:preview",
    "start": "nx serve",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true nx vite:build --config vitest.config.ts && pnpm dlx serve -s dist",
    "lighthouse": "pnpm build:prod && pnpm dlx serve -s dist & npx lighthouse http://localhost:3000 --view",
    "generate:pwa-icons": "bash ./scripts/generate-pwa-icons.sh",
    "test": "nx run saga-abilities:vite:test",
    "test:watch": "nx run saga-abilities:vite:test --watch",
    "test:coverage": "nx run saga-abilities:vite:test --coverage",
    "test:coverage:detailed": "nx run saga-abilities:vite:test --coverage --reporter=verbose",
    "test:coverage:html": "nx run saga-abilities:vite:test --coverage --reporter=html --reporter=lcov --outdir=coverage",
    "lint": "nx run-many --target=lint --all --verbose",
    "lint:fix": "nx run-many --target=lint --all --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@mantine/core": "^7.17.7",
    "@mantine/dates": "^7.17.7",
    "@mantine/ds": "^6.0.22",
    "@mantine/form": "^7.17.7",
    "@mantine/hooks": "^7.17.7",
    "@mantine/modals": "^7.17.7",
    "@mantine/notifications": "^7.17.7",
    "@tabler/icons-react": "^3.33.0",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-table": "^8.21.3",
    "@vitejs/plugin-react": "^4.7.0",
    "dayjs": "^1.11.13",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "localforage": "^1.10.0",
    "match-sorter": "^8.0.2",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2",
    "sort-by": "^1.2.0",
    "vite": "^6.3.6",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@nx/eslint": "^21.1.2",
    "@nx/js": "21.1.2",
    "@nx/react": "^21.1.2",
    "@nx/storybook": "21.1.2",
    "@nx/vite": "21.1.2",
    "@nx/web": "21.1.2",
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "^8.6.11",
    "@storybook/core-server": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.22.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.7",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@typescript-eslint/utils": "^8.32.1",
    "@vitest/coverage-v8": "3.1.4",
    "@vitest/ui": "^3.1.4",
    "canvas": "^3.1.2",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.7",
    "eslint": "^9.27.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "happy-dom": "^17.4.7",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "lighthouse": "^12.6.0",
    "nx": "21.1.2",
    "prettier": "^2.6.2",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.0",
    "storybook": "8.6.14",
    "terser": "^5.39.2",
    "ts-node": "10.9.1",
    "typescript": "^5.8.3",
    "vite-plugin-bundle-analyzer": "^0.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-preload": "^0.4.2",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.1.4"
  }
}